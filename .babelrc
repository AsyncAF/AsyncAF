{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "env": {
    "rollup": {
      "presets": [
        ["@babel/preset-env", {
            "modules": false,
            "targets": [
              "last 2 Chrome versions",
              "last 2 Safari versions",
              "last 2 iOS versions",
              "last 2 Firefox versions",
              "last 2 Edge versions"
            ],
            "shippedProposals": true
        }]
      ],
      "plugins": []
    }
  }
}
